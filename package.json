{
  "name": "nestjs-e2e",
  "version": "1.0.0",
  "description": "Set of helpers to easily write NestJS e2e tests",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -p tsconfig.json",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm test && pnpm run lint",
    "preversion": "pnpm run lint",
    "version": "pnpm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mehdibo/nestjs-e2e.git"
  },
  "bugs": {
    "url": "https://github.com/mehdibo/nestjs-e2e/issues"
  },
  "homepage": "https://github.com/mehdibo/nestjs-e2e#readme",
  "author": "Mehdi Bounya",
  "license": "MIT",
  "packageManager": "pnpm@8.14.3",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.3",
    "@nestjs/testing": "^10.3"
  }
}
